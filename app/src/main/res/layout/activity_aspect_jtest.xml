<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.aire.android.aspectJ.AspectJTestActivity">

    <TextView
        android:id="@+id/aspectj_start_webview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="200dp"
        android:onClick="startWebViewForAspectJ"
        android:paddingTop="50dp"
        android:paddingBottom="50dp"
        android:text="点击启动webview" />

    <TextView
        android:id="@+id/aspectj_start_webview_java"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:onClick="startWebViewForAspectJJava"
        android:paddingTop="50dp"
        android:paddingBottom="50dp"
        android:text="点击启动webview" />

</LinearLayout>